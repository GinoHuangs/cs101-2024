#include <stdio.h>

int main()
{
    FILE *inputfile, *outputfile;
    char line[1000];
    inputfile = fopen("main2.c","r");
    if(inputfile == NULL){
        printf("ERROR");
        return 1;
    }
    outputfile = fopen("main2.txt","w");
     if(outputfile == NULL){
        printf("ERROR");
        return 1;
    }
    while(fgets(line,sizeof(line),inputfile) != NULL){
        fputs(line,outputfile);
    }
    fclose(inputfile);
    fclose(outputfile);

    return 0;
}
